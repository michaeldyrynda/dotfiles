{
    "title": "Disable WifiDiagnostics shortcuts (Free up Hyper-W)",
    "rules": [
        {
            "description": "Disable command+control+option+shift+, which triggers WiFi logging.",
            "manipulators": [
                {
                    "from": {
                        "key_code": "comma",
                        "modifiers": {
                          "mandatory": [
                            "command",
                            "control",
                            "option",
                            "shift"
                          ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "f13"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
